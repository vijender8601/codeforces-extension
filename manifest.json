{
    "name" : "CF Contest Update",
    "description" : "An extension which show upcoming Contest.",
    "version" : "1.0",
    "manifest_version" : 3,

    "background" : {
        "service_worker" : "background.js"
    },

    "permissions" : [
        "activeTab"
    ],

    "action" : {
        "default_popup" : "index.html"
    },

    "icons" : {
        "16" : "/images/codeforces_16.png",
        "32" : "/images/codeforces_32.png",
        "48" : "/images/codeforces_48.png"
    }


}